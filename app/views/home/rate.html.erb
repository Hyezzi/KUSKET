<div class="container">
<h1 class="text-center mt-5">경쟁률 조회</h1>

<table class="table">
    <thead>
    <tr>
        <th>캠퍼스</th>
        <th>학수번호</th>
        <th>분반</th>
        <th>이수구분</th>
        <th>교과목명</th>
        <th>담당교수</th>
        <th>학점</th>
        <th>선택</th>
    </tr>
    </thead>
    <tbody>
    
    
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.campus %></td>
        <td><%= course.haksu %></td>
        <td><%= course.bunban %></td>
        <td><%= course.isu_type %></td>
        <td><%= course.course_name %></td>
        <td><%= course.lecturer %></td>
        <td><%= course.credit %></td>
        <td><%= %></td>
      </tr>
    <% end %>
   
    </tbody>
</table>
</div>
